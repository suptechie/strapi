{
  "routes": [
    {
      "method": "GET",
      "path": "/",
      "handler": "Admin.index",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/plugins",
      "handler": "Admin.plugins",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/:plugin/:file",
      "handler": "Admin.pluginFile",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/:file",
      "handler": "Admin.file",
      "config": {
        "policies": []
      }
    }, {
      "method": "GET",
      "path": "/*",
      "handler": "Admin.index",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/plugins/uninstall/:plugin",
      "handler": "Admin.uninstallPlugin",
      "config": {
        "policies": []
      }
    }
  ]
}
