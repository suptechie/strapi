{
  "session": {
    "enabled": false,
    "client": "redis",
    "settings": {
      "host": "localhost",
      "port": 6379,
      "database": 10,
      "password": ""
    },
    "key": "strapi.sid",
    "prefix": "strapi:sess:",
    "ttl": 86400000,
    "rolling": false,
    "secretKeys": ["mySecretKey1", "mySecretKey2"],
    "cookie": {
      "path": "/",
      "httpOnly": true,
      "maxAge": 86400000,
      "rewrite": true,
      "signed": false
    }
  },
  "logger": {
    "logger": {
      "level": "info",
      "exposeInContext": true,
      "requests": false
    }
  },
  "parser": {
    "enabled": true,
    "multipart": true
  },
  "router": {
    "prefix": ""
  }
}
