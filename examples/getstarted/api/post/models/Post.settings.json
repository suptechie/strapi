{
  "connection": "default",
  "collectionName": "posts",
  "info": {
    "name": "post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "title": {
      "type": "string",
      "default": "my super title"
    },
    "cover": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "metas": {
      "type": "component",
      "component": "default.metas",
      "min": 1,
      "repeatable": true
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "blog.code_snippet",
        "blog.quote",
        "blog.rich_text",
        "blog.slider",
        "blog.text_with_image"
      ],
      "min": 2
    },
    "contente": {
      "type": "dynamiczone",
      "components": [
        "blog.code_snippet",
        "blog.quote",
        "blog.rich_text",
        "blog.slider",
        "blog.text_with_image"
      ],
      "max": 3
    }
  }
}
