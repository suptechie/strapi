{
  "connection": "default",
  "collectionName": "articles",
  "info": {
    "name": "article",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "title": {
      "type": "string",
      "default": "soupette",
      "required": true,
      "unique": true
    },
    "content": {
      "type": "text"
    },
    "number": {
      "type": "integer"
    },
    "date": {
      "type": "date"
    },
    "enum": {
      "enum": [
        "morning",
        "noon"
      ],
      "type": "enumeration",
      "default": "noon"
    },
    "pic": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "bool": {
      "type": "boolean"
    },
    "tags": {
      "collection": "tag",
      "via": "article"
    },
    "json": {
      "type": "json"
    },
    "pics": {
      "collection": "file",
      "via": "related",
      "plugin": "upload"
    }
  }
}