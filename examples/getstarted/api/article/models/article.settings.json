    },
    "content2repet": {
      "type": "component",
      "repeatable": true,
      "component": "blog.content"
    },
    "dz": {
      "type": "dynamiczone",
      "components": [
        "blog.content",
        "blog.sub"
      ]
    },
    "address": {
      "model": "address"
    },
    "address2": {
      "model": "address"
    }
  }
}
