{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "articles",
  "info": {
    "name": "article"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "body": {
      "type": "dynamiczone",
      "components": [
        "article.test-compo"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "pics": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    }
  }
}
