{
  "connection": "default",
  "collectionName": "articles",
  "info": {
    "name": "articles",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "content2": {
      "type": "text"
    },
    "posts": {
      "collection": "post",
      "via": "articles"
    },
    "title": {
      "type": "string"
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "ingredients": {
      "type": "group",
      "groups": ["ingredients"],
      "required": true,
      "repeatable": true,
      "min": 2,
      "max": 3
    }
  }
}
