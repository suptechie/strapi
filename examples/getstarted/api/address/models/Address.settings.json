{
  "connection": "default",
  "collectionName": "addresses",
  "info": {
    "name": "address",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": ["created_at", "updated_at"],
    "comment": ""
  },
  "attributes": {
    "full_name": {
      "type": "string"
    },
    "postal_code": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "geolocation": {
      "type": "json",
      "required": true
    },
    "country": {
      "model": "country"
    },
    "cover": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "images": {
      "collection": "file",
      "via": "related",
      "plugin": "upload"
    }
  }
}
