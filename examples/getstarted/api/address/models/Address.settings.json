{
  "kind": "collectionType",
  "collectionName": "addresses",
  "info": {
    "name": "address",
    "description": ""
  },
  "options": {
    "draftAndPublish": true,
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "postal_coder": {
      "type": "string",
      "minLength": 3,
      "default": "test"
    },
    "categories": {
      "collection": "category",
      "via": "addresses",
      "dominant": true
    },
    "cover": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "city": {
      "type": "string",
      "required": true
    },
    "likes": {
      "collection": "like",
      "via": "address"
    },
    "dish": {
      "type": "component",
      "repeatable": true,
      "component": "default.dish",
      "required": true,
      "min": 1
    },
    "test": {
      "collection": "category"
    },
    "enum": {
      "type": "enumeration",
      "enum": [
        "un",
        "deux"
      ]
    },
    "address": {
      "model": "address"
    },
    "admin_user": {
      "plugin": "admin",
      "model": "user"
    }
  }
}
