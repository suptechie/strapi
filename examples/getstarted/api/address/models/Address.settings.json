{
  "connection": "default",
  "collectionName": "addresses",
  "info": {
    "name": "address",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "geolocation": {
      "type": "json",
      "required": true
    },
    "city": {
      "type": "string"
    },
    "postal_coder": {
      "type": "string"
    },
    "category": {
      "model": "category"
    },
    "cover": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "images": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "full_name": {
      "type": "string"
    },
    "rrrr": {
      "type": "component",
      "repeatable": false,
      "component": "default.rrr"
    },
    "date": {
      "type": "date"
    },
    "datetime": {
      "type": "datetime"
    },
    "time": {
      "type": "time"
    }
  }
}
